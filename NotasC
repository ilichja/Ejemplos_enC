#include <stdio.h>
int main (){
    int opcion, i;
    printf("Que opcion te gustaria elegir\n");
    for (i=0; i<8; i++){
        printf("Opcion: %i\n", i);
    }
    scanf("%i", &opcion);
    if(opcion < 9){
        printf("Elegiste la opcion %i", opcion);
    }
    else{
        printf("no existe esa opcion");
    }
}
